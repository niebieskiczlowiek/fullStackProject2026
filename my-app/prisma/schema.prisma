generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider  = "postgresql"
  relationMode = "prisma"
}

model User {
  id          Int       @id @default(autoincrement())
  first_name  String
  last_name   String
  username    String    @unique
  password    String
  email       String    @unique
  created_at  DateTime  @default(now())
}
